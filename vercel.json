{
  "version": 2,
  "builds": [
    {
      "src": "web-interface/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web-interface/server.js"
    }
  ],
  "functions": {
    "web-interface/server.js": {
      "maxDuration": 300
    }
  },
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "false"
  }
} 